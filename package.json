{
  "name": "dscsport",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint src __tests__ --fix --ext .js,.ts,.tsx && yarn run format",
    "start": "react-native start",
    "test": "jest",
    "coverage": "yarn test --coverage --watchAll --collectCoverageFrom='src/**/*.{js,jsx,ts,tsx}'",
    "android:dev": "react-native run-android --mode=developmentDebug --appId=com.dscsport.development",
    "android:stag": "react-native run-android --mode=stagingRelease --appId=com.dscsport.staging",
    "android:prod": "react-native run-android --mode=productionRelease --appId=com.dscsport",
    "android:prod:debug": "react-native run-android --mode=productionDebug --appId=com.dscsport",
    "build:dev:apk": "cd android && ./gradlew clean && ./gradlew assembleDevelopmentRelease && cd .. && open ./android/app/build/outputs",
    "build:stag:apk": "cd android && ./gradlew clean && ./gradlew assembleStagingRelease && cd .. && open ./android/app/build/outputs",
    "build:prod:apk": "cd android && ./gradlew clean && ./gradlew assembleProductionRelease && cd .. && open ./android/app/build/outputs",
    "prepare": "husky",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.4",
    "@hookform/resolvers": "^3.7.0",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.10.0",
    "@reduxjs/toolkit": "^2.2.6",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "i18next": "^23.11.5",
    "jetifier": "^2.0.0",
    "react": "18.2.0",
    "react-hook-form": "^7.52.1",
    "react-i18next": "^14.1.2",
    "react-native": "0.73.8",
    "react-native-config": "^1.5.2",
    "react-native-device-info": "^11.1.0",
    "react-native-error-boundary": "^1.2.4",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "2.15.0",
    "react-native-location": "^2.5.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "3.9.0",
    "react-native-safe-area-context": "^4.10.7",
    "react-native-screens": "^3.32.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-webview": "^13.10.5",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@serverless-guru/prettier-plugin-import-order": "^0.4.2",
    "@testing-library/react-native": "^12.5.1",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "husky": "^9.0.11",
    "jest": "^29.2.1",
    "jest-esm-transformer": "^1.0.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
